<!DOCTYPE html>
<html lang = "en">
	
	<head>
	
		<title> GQMaps Change Password </title>
		
		<!--  Custom Styles -->
		<link href = "css/dashboard.css" rel = "stylesheet" type = "text/css" />
		<link href = "css/timeout.css" rel = "stylesheet" type = "text/css" />
		
		<!-- Predefined Styles -->
		<link href = 'http://fonts.googleapis.com/css?family=Open+Sans:400,600,700' rel = 'stylesheet' type = 'text/css' />
		
		<!-- Predefined Script -->
		<script type = "text/javascript" src = "http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"> </script>
		
		<!--/custom scripts -->
		<script src = "js/jstorage.js"> </script>
		<script src = "js/rest_service.js"> </script>
		<script src = "js/general.js"> </script>
		<script src = "js/change_password.js"> </script>
		<script src = "js/check_answer.js"> </script>
		
	</head>
	
	<body>
	
		<!-- Timeout Div -->
		<div id = "idletimeout">
			Logging off in <span> countdown place holder </span>&nbsp;seconds due to inactivity.
			<a id = "idletimeout-resume" href = "#"> Click here to continue </a>.
		</div>
		
		<!-- Fixed top -->
		<div id = "top">
			<div class = "fixed">
				<a href = "#" title = "" class = "logo"></a>
				<img class = "logo-menu" src = "images/image001.jpg"></img>
			</div>
		</div>
		<!-- /Fixed top -->
		
		<!-- Content container -->
		<div id = "container">
			<!-- Content wrapper -->
			<div class = "wrapper" style = "margin-left:auto; margin-right:auto; width:70%;">
			
				<br/><br/>
				
				<h5 style = "text-align:center" class = "widget-name" >
					<i style = "display:block; position:absolute; left:370px;" class = "icon-ok"></i>
				 	Change Password
				</h5>
				
				<br/><br/>
				
				<!-- Form validation -->
				<form id = "frmChangePwd" class = "form-horizontal">
					<fieldset>
					
						<!-- Widget starts -->
						<div class = "widget">
						
							<div class = "well row-fluid">
							
								<div class = "control-group">
								
									<label class = "control-label"> User Id </label>
									
									<div class = "controls">
										<label id = "tdUserID" class = "control-label"></label>
									</div>
									
								</div>
								
								<div class = "control-group">
								
									<label for = "cmbQues1" class = "control-label"> Security Question 1 
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<select name = "cmbQues1" id = "cmbQues1" class = "validate[required] styled " 
										data-prompt-position = "topLeft:-1,-5">
										</select>
									</div>
								
								</div>
								
								<div class = "control-group">
								
									<label for = "txtAns1" class = "control-label"> Answer 1
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<input type = "text" style = "width:50%;" name = "txtAns1" id = "txtAns1" maxlength = "20" 
										class = "validate[required]" placeholder = "Answer must be in one word and less than 10 characters" />
									</div>
									
								</div>
								
								<div class = "control-group">
									
									<label for = "cmbQues2" class = "control-label"> Security Question 2
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<select name = "cmbQues2" id = "cmbQues2" class = "validate[required] styled " 
										data-prompt-position = "topLeft:-1,-5">
										</select>
									</div>
								
								</div>
								
								<div class = "control-group">
								
									<label for = "txtAns2" class = "control-label"> Answer 2
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<input type = "text" style = "width:50%;" name = "txtAns2" id = "txtAns2" maxlength = "20" 
										class = "validate[required]" placeholder = "Answer must be in one word and less than 10 characters" />
									</div>
									
								</div>
								
								<div class = "control-group">
								
									<label for = "txtNewPwd" class = "control-label"> New Password
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<input type = "password" name = "txtNewPwd" id = "txtNewPwd" maxlength = "45" style = "width:50%;" 
										class = "validate[required]" 
										placeholder = "6-12 characters with at least one special,numeric,uppercase and lowercase character"/>
									</div>
								
								</div>
								
								<div class = "control-group">
								
									<label for = "txtConfPwd" class = "control-label"> Confirm Password
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<input type = "password" name = "txtConfPwd" id = "txtConfPwd" maxlength = "45" style = "width:50%;" 
										class = "validate[required]" 
										placeholder = "6-12 characters with at least one special,numeric,uppercase and lowercase character" />
									</div>
								
								</div>
								
								<div class = "control-group" >
								
									<span class = "label label-info" style = "line-height:20px;">
										<input type = "checkbox" name = "chkChageQA" id = "chkChageQA" > Change Security Questions
									</span>
									
								</div>
								
								<div class = "control-group">
								
									<label for = "cmbChangeQues1" class = "control-label"> Security Question 1
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<select name = "cmbChangeQues1" id = "cmbChangeQues1" class = "validate[required] styled " 
										data-prompt-position = "topLeft:-1,-5">
										</select>
									</div>
								
								</div>
								
								<div class = "control-group">
								
									<label for = "txtChangeAns1" class = "control-label"> Answer 1
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<input type = "text" style = "width:50%;" name = "txtChangeAns1" id = "txtChangeAns1" maxlength="20" 
										class = "validate[required]" 
										placeholder = "Answer must be in one word and less than 10 characters" />
									</div>
								
								</div>
								
								<div class = "control-group">
								
									<label for = "cmbChangeQues2" class = "control-label"> Security Question 2
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<select name = "cmbChangeQues2" id = "cmbChangeQues2" class = "validate[required] styled " 
										data-prompt-position = "topLeft:-1,-5">
										</select>
									</div>
								
								</div>
								
								<div class = "control-group">
								
									<label for = "txtChangeAns2" class = "control-label"> Answer 2
										<span class = "text-error">*</span>
									</label>
									
									<div class = "controls">
										<input type = "text" style = "width:50%;" name = "txtChangeAns2" id = "txtChangeAns2" 
										maxlength = "20" class = "validate[required]" 
										placeholder = "Answer must be in one word and less than 10 characters" />
									</div>
								
								</div>
								
								<div class = "form-actions align-right">
									
									<button class = "btn btn-warning" type = "button" style = "position:relative; right:680px;" 
									onclick = "gotoLogin()"> Back to Login </button>
									<button id = "submit" type = "button" class = "btn btn-info"> Submit </button>
									<button type = "reset" class = "btn">Reset </button>
								
								</div>
							
							</div>
							
						</div>
						<!-- Widget ends -->
					</fieldset>
				
				</form>
				<!-- /Form validation -->
			</div>
			<!-- /Content wrapper -->
		</div>
		<!-- /Content container -->
		
	</body>

</html>

	<!-- Mask to cover the whole screen -->
	<div id = "mask"> </div>
		 	
	<script src = "js/jquery.idletimer.js" type = "text/javascript" > </script>
	<script src = "js/jquery.idletimeout.js" type = "text/javascript" > </script>
        
	<script type = "text/javascript" >
		$.idleTimeout('#idletimeout', '#idletimeout a', {
		idleAfter : $.jStorage.get("jsTimeout"),
        onTimeout : function() {
            $(this).slideUp();
            window.location = "login.html";
        },
        onIdle : function() {
            var maskHeight = $(document).height();
            var maskWidth = $(window).width();
            $('#mask').css({'width':maskWidth,'height':maskHeight});
            $('#mask').fadeIn(1000);	
            $('#mask').fadeTo("slow",0.8);	
            $(this).slideDown(); // show the warning bar
        },
        onCountdown : function(counter) {
            $(this).find("span").html(counter); // update the counter
        },
        onResume : function() {
            $(this).slideUp(); // hide the warning bar
            $('#mask').hide();
            $('.window').hide();
        }
    });
	</script>